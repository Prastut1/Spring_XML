pipeline{
    
    agent any
    
    stages{
        
        stage ('Compile Stage'){
                  
                  steps{
                      
                      withMaven(maven :'Maven3'){
                          sh 'mvn clean install'
                      }

                  }
                  

              }
        stage ('Testing Stage'){
        
        		                  steps{
                      
                      withMaven(maven :'Maven3'){
                          sh 'mvn test'
                      }

                  }
                  
                  
              }


        
    }

}
